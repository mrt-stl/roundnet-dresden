{
    "version": 2,
    "name": "derjugendchor",
    "alias": "derjugendchor",
    "public": false,
    "regions": [
        "all"
    ],
    "builds": [
        {
            "src": "next.config.js",
            "use": "@now/next"
        },
        {
            "src": "static/*",
            "use": "@now/static"
        }
    ],
    "build": {
        "env": {
            "url": "https://derjugendchor.de",
            "name": "derjugendchor",
            "prismic_endpoint": "https://derjugendchorde.cdn.prismic.io/api/v2",
            "prismic_access_token": "MC5ZQ0VJUVJFQUFDZ0FxNjVI.77-977-9H3fvv73vv71977-9NhXvv70c77-9XBMJTzfvv70a77-977-9BVt_Je-_ve-_vVLvv70KRA",
            "nav": "[ {\"name\": \"Start\", \"link\": \"/\"}",
            "font": "{\"url\": \"https://use.typekit.net/zvs7feh.css\", \"name\": \"neue-haas-unica\" }",
            "cookie": "{\"link\": \"\"}",
            "colors": "{ \"primary\": \"#000000\", \"secondary\": \"#ffffff\", \"accent\": \"#000000\" }",
            "dark_mode": "on",
            "caching_duration": "0",
            "project_id": "derjugendchor",
            "show_banner": "on",
            "shopify_acces_token": "",
            "shopify_store_domain": "",
            "ga_id": "",
            "use_shop_view": "false"
        }
    },
    "routes": [
        {
            "src": "^/robots.txt",
            "dest": "/static/robots.txt"
        },
        {
            "src": "^/sitemap.xml",
            "dest": "/api/sitemap"
        },
        {
            "src": "/en/(?<id>[^/]+)$",
            "dest": "/index?id=$id&lang=en"
        },
        {
            "src": "/en/",
            "dest": "/index?id=home&lang=en"
        },
        {
            "src": "/(?<id>[^/]+)$",
            "dest": "/index?id=$id"
        }
    ]
}