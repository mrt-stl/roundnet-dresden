{
    "version": 2,
    "name": "power4africa",
    "alias": "power4africa",
    "public": false,
    "regions": [
        "all"
    ],
    "builds": [
        {
            "src": "next.config.js",
            "use": "@now/next"
        },
        {
            "src": "static/*",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "^/robots.txt",
            "dest": "/static/robots.txt"
        },
        {
            "src": "^/sitemap.xml",
            "dest": "/api/sitemap"
        },
        {
            "src": "/projekte/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=projekte"
        },
        {
            "src": "/(?<id>[^/]+)$",
            "dest": "/index?id=$id"
        },
        {
            "src": "/ar/(?<id>[^/]+)$",
            "dest": "/index?id=$id&lang=ar"
        },
        {
            "src": "/ar/",
            "dest": "/index?id=home&lang=ar"
        }
    ]
}