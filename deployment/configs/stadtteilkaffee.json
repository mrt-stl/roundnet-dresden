{
    "version": 2,
    "name": "stadtteilkaffee",
    "alias": "stadtteilkaffee.de",
    "public": false,
    "regions": [
        "all"
    ],
    "builds": [
        {
            "src": "next.config.js",
            "use": "@vercel/next"
        },
        {
            "src": "static/*",
            "use": "@vercel/static"
        }
    ],
    "build": {
        "env": {
            "url": "https://stadtteilkaffee.de/",
            "name": "stl",
            "prismic_endpoint": "https://stadtteilkaffee.cdn.prismic.io/api/v2",
            "prismic_access_token": "MC5YN09MOWhJQUFDQUFmZl9H.VkTvv70N77-9be-_vSTvv71F77-9Y--_vQvvv71M77-9FzDvv73vv73vv73vv73vv70kXwIl77-9O--_vXk",
            "nav": "[ {\"name\": \"Start\", \"link\": \"/\"} ]",
            "font": "{\"url\": \"https://use.typekit.net/zvs7feh.css\", \"name\": \"neue-haas-unica\" }",
            "cookie": "{\"link\": \"https://stadtteilliebe.de/datenschutz\"}",
            "colors": "{ \"primary\": \"#744244\", \"secondary\": \"#ffffff\", \"accent\": \"#D47479\" }",
            "dark_mode": "on",
            "caching_duration": "0",
            "project_id": "stadtteilkaffee",
            "show_banner": "off",
            "shopify_acces_token": "",
            "shopify_store_domain": "",
            "ga_id": "",
            "use_shop_view": "false"
        }
    },
    "routes": [
        {
            "src": "^/robots.txt",
            "dest": "/static/robots.txt"
        },
        {
            "src": "^/sitemap.xml",
            "dest": "/api/sitemap"
        },
        {
            "src": "/en/(?<id>[^/]+)$",
            "dest": "/index?id=$id&lang=en"
        },
        {
            "src": "/en/",
            "dest": "/index?id=home&lang=en"
        },
        {
            "src": "/(?<id>[^/]+)$",
            "dest": "/index?id=$id"
        }
    ]
}