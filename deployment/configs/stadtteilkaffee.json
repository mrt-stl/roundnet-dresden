            "ACCESS_TOKEN": "MC5YN09MOWhJQUFDQUFmZl9H.VkTvv70N77-9be-_vSTvv71F77-9Y--_vQvvv71M77-9FzDvv73vv73vv73vv73vv70kXwIl77-9O--_vXk",
            "NAV": "[ {\"name\": \"Start\", \"link\": \"/\"}, {\"name\": \"Das Caf√©\", \"link\": \"/projekte\"}, {\"name\": \"Wir\", \"link\": \"/wir\"}, {\"name\": \"Blog\", \"link\": \"/blog\"} ]",
