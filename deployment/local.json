{
    "version": 2,
    "name": "local",
    "public": false,
    "regions": [
        "dev1"
    ],
    "builds": [
        {
            "src": "next.config.js",
            "use": "@now/next"
        },
        {
            "src": "static/*",
            "use": "@now/static"
        }
    ],
    "build": {
        "env": {
            "URL": "https://stadtteilliebe.de/",
            "NAME": "stl",
            "PRISMIC_ENDPOINT": "https://stadtteilliebe2.cdn.prismic.io/api/v2",
            "ACCESS_TOKEN": "MC5YNVFWYnhBQUFDSUFiZ2Za.Ig3vv70Q77-977-977-9N--_ve-_ve-_vVDvv73vv70M77-977-977-977-977-9de-_ve-_ve-_ve-_ve-_vUoB77-977-9Dmo",
            "NAV": "[ {\"name\": \"Start\", \"link\": \"/\"}, {\"name\": \"Agentur\", \"link\": \"/agentur\"}, {\"name\": \"Blog\", \"link\": \"/blog\"}, {\"name\": \"Projekte\", \"link\": \"/projekte\"} ]",
            "FONT": "{\"url\": \"https://use.typekit.net/zvs7feh.css\", \"name\": \"neue-haas-unica\" }",
            "COOKIE": "{\"link\": \"https://stadtteilliebe.de/datenschutz\"}",
            "COLORS": "{ \"primary\": \"\", \"secondary\": \"\", \"accent\": \"\" }",
            "DARK_MODE": "auto",
            "CACHING_TIME": "0",
            "PROJECT_ID": "stl",
            "HAS_BANNER": "off",
            "USE_SHOP_VIEW": "",
            "SHOPIFY_STORE_DOMAIN": "",
            "SHOPIFY_ACCESS_TOKEN": ""
        }
    },
    "routes": [
        {
            "src": "^/robots.txt",
            "dest": "/static/robots.txt"
        },
        {
            "src": "^/sitemap.xml",
            "dest": "/api/sitemap"
        },
        {
            "src": "/jobs/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=jobs"
        },
        {
            "src": "/en/blog/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=blog&lang=en"
        },
        {
            "src": "/blog/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=blog"
        },
        {
            "src": "/en/(?<id>[^/]+)$",
            "dest": "/index?id=$id&lang=en"
        },
        {
            "src": "/en/",
            "dest": "/index?id=home&lang=en"
        },
        {
            "src": "/cart",
            "dest": "/cart"
        },
        {
            "src": "/(?<id>[^/]+)$",
            "dest": "/index?id=$id"
        }
    ]
}