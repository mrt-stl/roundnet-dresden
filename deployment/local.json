    ],
    "builds": [
        {
            "src": "next.config.js",
            "use": "@now/next"
        },
        {
            "src": "static/*",
            "use": "@now/static"
        }
    ],
    "build": {
        "env": {
            "URL": "https://stadtteilliebe.de",
            "NAME": "stl",
            "PRISMIC_ENDPOINT": "https://stadtteilliebe2.cdn.prismic.io/api/v2",
            "ACCESS_TOKEN": "MC5YTkFjbnhFQUFGOElqbjNx.FH7vv71m77-977-9CWvvv73vv71-U2bvv73vv71uOe-_vUjvv73vv73vv70G77-9Ue-_ve-_ve-_ve-_vW_vv71U",
            "NAV": "[ {\"name\": \"Stadtteilliebe\", \"link\": \"/\"}, {\"name\": \"Leistungen\", \"link\": \"/leistungen\"}, {\"name\": \"Projekte\", \"link\": \"/projekte\"}, {\"name\": \"Blog\", \"link\": \"/blog\"}, {\"name\": \"Kontakt\", \"link\": \"/kontakt\"}]",
            "FONT": "{\"url\": \"https://use.typekit.net/zvs7feh.css\", \"name\": \"neue-haas-unica\" }",
