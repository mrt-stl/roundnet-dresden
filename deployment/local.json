{
    "version": 2,
    "name": "local",
    "public": false,
    "regions": [
        "dev1"
    ],
    "builds": [
        {
            "src": "next.config.js",
            "use": "@now/next"
        },
        {
            "src": "static/*",
            "use": "@now/static"
        }
    ],
    "build": {
        "env": {
            "url": "https://stadtteilliebe.de/",
            "name": "stl",
            "prismic_endpoint": "https://stadtteilliebe2.cdn.prismic.io/api/v2",
            "prismic_access_token": "MC5YNVFWYnhBQUFDSUFiZ2Za.Ig3vv70Q77-977-977-9N--_ve-_ve-_vVDvv73vv70M77-977-977-977-977-9de-_ve-_ve-_ve-_ve-_vUoB77-977-9Dmo",
            "nav": "[ {\"name\": \"Start\", \"link\": \"/\"}, {\"name\": \"Projekte\", \"link\": \"/projekte\"}, {\"name\": \"Wir\", \"link\": \"/wir\"}, {\"name\": \"Blog\", \"link\": \"/blog\"}, {\"name\": \"Unser%20Blog\", \"link\": \"/blog\"} ]",
            "font": "{\"url\": \"https://use.typekit.net/zvs7feh.css\", \"name\": \"neue-haas-unica\" }",
            "cookie": "{\"link\": \"https://stadtteilliebe.de/datenschutz\"}",
            "color": "{ \"primary\": \"#8345e5\", \"secondary\": \"#9aa7c1\", \"accent\": \"#ec29b9\" }",
            "dark_mode": "auto",
            "caching_duration": "0",
            "project_id": "stl",
            "show_banner": "off",
            "use_shop_view": "false",
            "shopify_store_domain": "",
            "shopify_access_token": "",
            "ga_id": ""
        }
    },
    "routes": [
        {
            "src": "^/robots.txt",
            "dest": "/static/robots.txt"
        },
        {
            "src": "^/sitemap.xml",
            "dest": "/api/sitemap"
        },
        {
            "src": "/jobs/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=jobs"
        },
        {
            "src": "/en/blog/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=blog&lang=en"
        },
        {
            "src": "/blog/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=blog"
        },
        {
            "src": "/en/(?<id>[^/]+)$",
            "dest": "/index?id=$id&lang=en"
        },
        {
            "src": "/en/",
            "dest": "/index?id=home&lang=en"
        },
        {
            "src": "/cart",
            "dest": "/cart"
        },
        {
            "src": "/(?<id>[^/]+)$",
            "dest": "/index?id=$id"
        }
    ]
}