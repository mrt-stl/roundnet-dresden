{
    "version": 2,
    "name": "local",
    "public": false,
    "regions": [
        "dev1"
    ],
    "builds": [
        {
            "src": "next.config.js",
            "use": "@now/next"
        },
        {
            "src": "static/*",
            "use": "@now/static"
        }
    ],
    "build": {
        "env": {
            "URL": "https://standard.stl-dev.now.sh/",
            "NAME": "standard-stl",
            "PRISMIC_ENDPOINT": "https://shiningwhite.cdn.prismic.io/api/v2",
            "ACCESS_TOKEN": "MC5YbkR0amhJQUFDa0FjVzBx.77-977-977-977-977-9QEDvv70CTO-_vWE077-9ABgdTO-_vT8YMe-_ve-_vToq77-977-977-9P--_vWw",
            "NAV": "[ {\"name\": \"Start\", \"link\": \"/\"}, {\"name\": \"Shop\", \"link\": \"/home\"} ]",
            "FONT": "{\"url\": \"https://use.typekit.net/zvs7feh.css\", \"name\": \"neue-haas-unica\" }",
            "COOKIE": "{\"link\": \"https://stadtteilliebe.de/datenschutz\"}",
            "COLORS": "{ \"primary\": \"\", \"secondary\": \"\", \"accent\": \"\" }",
            "DARK_MODE": "auto",
            "CACHING_TIME": "0",
            "PROJECT_ID": "stl",
            "HAS_BANNER": "on",
            "USE_SHOP_VIEW": "true",
            "SHOPIFY_STORE_DOMAIN": "stadtteilshop.myshopify.com",
            "SHOPIFY_ACCESS_TOKEN": "8a1375fb56d36b6ed4aed75934b9051e"
        }
    },
    "routes": [
        {
            "src": "^/robots.txt",
            "dest": "/static/robots.txt"
        },
        {
            "src": "^/sitemap.xml",
            "dest": "/api/sitemap"
        },
        {
            "src": "/jobs/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=jobs"
        },
        {
            "src": "/en/blog/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=blog&lang=en"
        },
        {
            "src": "/blog/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=blog"
        },
        {
            "src": "/en/(?<id>[^/]+)$",
            "dest": "/index?id=$id&lang=en"
        },
        {
            "src": "/en/",
            "dest": "/index?id=home&lang=en"
        },
        {
            "src": "/cart",
            "dest": "/cart"
        },
        {
            "src": "/(?<id>[^/]+)$",
            "dest": "/index?id=$id"
        }
    ]
}