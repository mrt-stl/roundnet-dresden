{
    "version": 2,
    "name": "local",
    "public": false,
    "regions": [
        "dev1"
    ],
    "builds": [
        {
            "src": "next.config.js",
            "use": "@now/next"
        },
        {
            "src": "static/*",
            "use": "@now/static"
        }
    ],
    "build": {
        "env": {
            "URL": "https://standard.stl-dev.now.sh/",
            "NAME": "standard-stl",
            "PRISMIC_ENDPOINT": "https://standard.cdn.prismic.io/api/v2",
            "ACCESS_TOKEN": "MC5YTWJpUFJFQUFDSUF0MzB2.77-977-977-977-9ae-_ve-_ve-_ve-_ve-_vWzvv70a77-9Yu-_vRvvv73vv71m77-977-977-9fGFE77-9Q0Hvv73vv70k",
            "NAV": "[ {\"name\": \"Start\", \"link\": \"/\"}, {\"name\": \"Home\", \"link\": \"/home\"} ]",
            "FONT": "{\"url\": \"https://use.typekit.net/zvs7feh.css\", \"name\": \"neue-haas-unica\" }",
            "COOKIE": "{\"link\": \"https://stadtteilliebe.de/datenschutz\"}",
            "COLORS": "{ \"primary\": \"\", \"secondary\": \"\", \"accent\": \"\" }",
            "DARK_MODE": "auto",
            "CACHING_TIME": "0",
            "PROJECT_ID": "stl",
            "HAS_BANNER": "on"
        }
    },
    "routes": [
        {
            "src": "^/robots.txt",
            "dest": "/static/robots.txt"
        },
        {
            "src": "^/sitemap.xml",
            "dest": "/api/sitemap"
        },
        {
            "src": "/jobs/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=jobs"
        },
        {
            "src": "/en/blog/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=blog&lang=en"
        },
        {
            "src": "/blog/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=blog"
        },
        {
            "src": "/en/(?<id>[^/]+)$",
            "dest": "/index?id=$id&lang=en"
        },
        {
            "src": "/en/",
            "dest": "/index?id=home&lang=en"
        },
        {
            "src": "/(?<id>[^/]+)$",
            "dest": "/index?id=$id"
        }
    ]
}