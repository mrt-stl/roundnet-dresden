{
    "version": 2,
    "name": "standard",
    "alias": "standard-stl",
    "public": false,
    "regions": [
        "all"
    ],
    "builds": [
        {
            "src": "next.config.js",
            "use": "@now/next"
        },
        {
            "src": "static/*",
            "use": "@now/static"
        }
    ],
    "build": {
        "env": {
            "URL": "https://standard.stl-dev.now.sh/",
            "PRISMIC_ENDPOINT": "https://standard.cdn.prismic.io/api/v2",
            "ACCESS_TOKEN": "MC5YTWJpUFJFQUFDSUF0MzB2.77-977-977-977-9ae-_ve-_ve-_ve-_ve-_vWzvv70a77-9Yu-_vRvvv73vv71m77-977-977-9fGFE77-9Q0Hvv73vv70k",
            "NAV": "[ {\"name\": \"Home\", \"link\": \"/\"} ]",
            "DARK_MODE": "auto",
            "HAS_BANNER": "0",
            "CACHING_TIME": "0"
        }
    },
    "routes": [
        {
            "src": "^/robots.txt",
            "dest": "/static/robots.txt"
        },
        {
            "src": "^/sitemap.xml",
            "dest": "/api/sitemap"
        },
        {
            "src": "/blog/(?<id>[^/]+)$",
            "dest": "/index?id=$id&type=blog"
        },
        {
            "src": "/(?<id>[^/]+)$",
            "dest": "/index?id=$id"
        }
    ]
}