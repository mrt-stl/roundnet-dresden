{
    "name": "tukan-platform",
    "version": "1.0.0",
    "engines": {
        "node": ">=10.0.0"
    },
    "scripts": {
        "local": "now dev -A deployment/local.json",
        "build": "next build",
        "start": "next start -p $PORT",
        "deploy-dev": "node deployment/development.js",
        "deploy-beta": "node deployment/beta.js",
        "deploy-prod": "node deployment/prod.js",
        "now-build": "next build",
        "lint": "yarn eslint && yarn tslint",
        "eslint": "eslint .",
        "tslint": "tslint -c ./tslint.json '**/*.{ts,tsx}'",
        "test": "jest --verbose",
        "test-units": "jest --verbose ./units",
        "test-coverage": "jest --verbose --coverage",
        "analyze": "cross-env ANALYZE=true next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
    },
    "dependencies": {
        "accept-language-parser": "^1.5.0",
        "axios": "^0.19.0",
        "google-map-react": "^1.1.5",
        "html-react-parser": "^0.9.1",
        "next": "^9.1.7",
        "nodemailer": "^6.3.0",
        "now": "^16.7.0",
        "prismic-dom": "^2.1.0",
        "prismic-javascript": "^2.1.1",
        "react": "^16.12.0",
        "react-autosize-textarea": "^7.0.0",
        "react-burger-menu": "^2.6.11",
        "react-dom": "^16.12.0",
        "react-hook-inview": "^4.1.2",
        "react-lazy-load-image-component": "^1.3.2",
        "react-lazyload": "^2.6.2",
        "rollbar": "^2.12.2",
        "sitemap": "^5.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
        "@babel/plugin-proposal-optional-chaining": "^7.7.5",
        "@next/bundle-analyzer": "^9.1.7",
        "@types/google-map-react": "^1.1.3",
        "@types/jest": "^24.0.23",
        "@types/node": "^12.12.17",
        "@types/prismic-dom": "^2.1.1",
        "@types/react": "^16.9.16",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "cross-env": "^6.0.3",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "eslint": "^6.7.2",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-security": "^1.4.0",
        "firebase-admin": "^8.3.0",
        "jest": "^24.9.0",
        "react-addons-test-utils": "^15.6.2",
        "react-test-renderer": "^16.10.2",
        "ts-jest": "^24.2.0",
        "tslint": "^5.20.1",
        "tslint-config-security": "^1.16.0",
        "tslint-react": "^4.1.0",
        "typescript": "^3.7.3"
    }
}