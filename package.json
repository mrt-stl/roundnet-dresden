{
    "name": "tukan-platform",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=14.0.0"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start -p $PORT",
        "lint": "yarn run eslint .",
        "test": "jest --verbose && yarn test-e2e",
        "test-build": "COOKIE=\"{\"link\": \"https://stadtteilliebe.de/datenschutz\"}\" USE_SHOP_VIEW=\"true\" next build",
        "test-server": "next start -p 8080",
        "test-units": "jest --verbose ./units",
        "test-coverage": "jest --verbose --coverage",
        "test-e2e": "start-server-and-test test-server http://localhost:8080 cypress",
        "test-e2e-head": "start-server-and-test test-server http://localhost:8080 cypress-head",
        "cypress": "cypress run --headless --browser chrome",
        "cypress-head": "cypress run --browser chrome",
        "analyze": "cross-env ANALYZE=true next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
    },
    "dependencies": {
        "@types/styled-components": "^5.1.12",
        "accept-language-parser": "^1.5.0",
        "axios": "^0.21.1",
        "babel-plugin-styled-components": "^1.11.1",
        "google-map-react": "^2.1.9",
        "html-react-parser": "^1.2.4",
        "js-cookie": "^3.0.0",
        "next": "^11.0.1",
        "next-secure-headers": "^2.2.0",
        "prismic-dom": "^2.2.6",
        "prismic-javascript": "^3.0.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-hook-inview": "^4.1.2",
        "react-lazy-load-image-component": "^1.3.2",
        "react-lazyload": "^3.2.0",
        "rollbar": "^2.12.2",
        "sitemap": "^5.0.0",
        "styled-components": "^5.2.0",
        "styled-components-breakpoints": "^1.0.0",
        "vercel": "^20.1.3"
    },
    "devDependencies": {
        "@babel/core": "^7.13.10",
        "@next/bundle-analyzer": "^10.0.8",
        "@types/google-map-react": "^2.1.2",
        "@types/jest": "^27.0.0",
        "@types/node": "^16.4.13",
        "@types/prismic-dom": "^2.1.1",
        "@types/react": "^17.0.17",
        "@typescript-eslint/eslint-plugin": "^4.29.1",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^26.6.3",
        "cypress": "^6.6.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "eslint": "^7.32.0",
        "eslint-config-next": "^11.0.1",
        "jest": "^26.6.3",
        "now": "^19.2.0",
        "react-addons-test-utils": "^15.6.2",
        "react-test-renderer": "^17.0.1",
        "start-server-and-test": "^1.10.11",
        "ts-jest": "^26.5.3",
        "typescript": "^4.3.5"
    }
}
