{
    "name": "tukan-platform",
    "version": "1.0.0",
    "engines": {
        "node": ">=10.0.0"
    },
    "scripts": {
        "local": "vercel dev -A ./deployment/local.json",
        "build": "next build",
        "start": "next start -p $PORT",
        "deploy-dev": "node deployment/development.js",
        "deploy-beta": "node deployment/beta.js",
        "deploy-prod": "node deployment/prod.js",
        "deploy-extra": "node deployment/extra.js",
        "lint": "yarn eslint && yarn tslint",
        "eslint": "eslint .",
        "tslint": "tslint -c ./tslint.json '**/*.{ts,tsx}'",
        "test": "jest --verbose && yarn test-e2e",
        "test-build": "COOKIE=\"{\"link\": \"https://stadtteilliebe.de/datenschutz\"}\" USE_SHOP_VIEW=\"true\" next build",
        "test-server": "next start -p 8080",
        "test-units": "jest --verbose ./units",
        "test-coverage": "jest --verbose --coverage",
        "test-e2e": "start-server-and-test test-server http://localhost:8080 cypress",
        "test-e2e-head": "start-server-and-test test-server http://localhost:8080 cypress-head",
        "cypress": "cypress run --headless --browser chrome",
        "cypress-head": "cypress run --browser chrome",
        "analyze": "cross-env ANALYZE=true next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
    },
    "dependencies": {
        "@types/styled-components": "^5.1.4",
        "accept-language-parser": "^1.5.0",
        "axios": "^0.19.2",
        "babel-plugin-styled-components": "^1.11.1",
        "google-map-react": "^1.1.5",
        "html-react-parser": "^0.10.0",
        "local-storage": "^2.0.0",
        "next": "^9.3.1",
        "nodemailer": "^6.3.0",
        "prismic-dom": "^2.1.0",
        "prismic-javascript": "^2.7.0",
        "react": "^16.13.1",
        "react-autosize-textarea": "^7.0.0",
        "react-dom": "^16.13.1",
        "react-hook-inview": "^4.1.2",
        "react-lazy-load-image-component": "^1.3.2",
        "react-lazyload": "^2.6.2",
        "rollbar": "^2.12.2",
        "shopify-buy": "^2.9.0",
        "sitemap": "^5.0.0",
        "styled-components": "^5.2.0",
        "styled-components-breakpoints": "^1.0.0",
        "use-dark-mode": "^2.3.1",
        "vercel": "^20.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@next/bundle-analyzer": "^9.3.1",
        "@types/google-map-react": "^1.1.3",
        "@types/jest": "^24.0.23",
        "@types/node": "^13.1.6",
        "@types/prismic-dom": "^2.1.1",
        "@types/react": "^16.9.25",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "cross-env": "^6.0.3",
        "cypress": "^4.2.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "eslint": "^6.7.2",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-security": "^1.4.0",
        "firebase-admin": "^8.3.0",
        "jest": "^24.9.0",
        "react-addons-test-utils": "^15.6.2",
        "react-test-renderer": "^16.10.2",
        "start-server-and-test": "^1.10.11",
        "ts-jest": "^24.2.0",
        "tslint": "^5.20.1",
        "tslint-config-security": "^1.16.0",
        "tslint-react": "^4.1.0",
        "typescript": "^3.8.3"
    }
}
