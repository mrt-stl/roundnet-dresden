{
    "extends": [
        "tslint:recommended",
        "tslint-config-security",
        "tslint-react"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**",
            ".next/**"
        ]
    },
    "jsRules": {},
    "rules": {
        "semicolon": [
            true,
            "never"
        ],
        "quotemark": [
            true,
            "double"
        ],
        "trailing-comma": [
            false
        ],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "arrow-parens": [
            false
        ],
        "eofline": false,
        "max-line-length": [
            true,
            1000
        ],
        "linebreak-style": [
            true,
            "LF"
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "tsr-detect-non-literal-fs-filename": [
            false
        ],
        "jsx-alignment": false,
        "jsx-no-multiline-js": false,
        "jsx-boolean-value": false,
        "jsx-wrap-multiline": false
    },
    "rulesDirectory": []
}